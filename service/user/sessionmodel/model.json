{
  "$schema": "https://xorkevin.dev/forge/schema/modelschema.json",
  "models": {
    "session": {
      "model": {
        "constraints": [
          {
            "kind": "PRIMARY KEY",
            "columns": ["userid", "sessionid"]
          }
        ],
        "indicies": [
          {
            "name": "userid_time",
            "columns": [{"col": "userid"}, {"col": "time"}]
          }
        ]
      },
      "queries": {
        "Model": [
          {
            "kind": "getoneeq",
            "name": "ByUserSession",
            "conditions": [{"col": "userid"}, {"col": "sessionid"}]
          },
          {
            "kind": "updeq",
            "name": "ByUserSession",
            "conditions": [{"col": "userid"}, {"col": "sessionid"}]
          },
          {
            "kind": "deleq",
            "name": "ByUserSession",
            "conditions": [{"col": "userid"}, {"col": "sessionid"}]
          },
          {
            "kind": "deleq",
            "name": "ByUserSessions",
            "conditions": [
              {"col": "userid"},
              {"col": "sessionid", "cond": "in"}
            ]
          },
          {
            "kind": "deleq",
            "name": "ByUserid",
            "conditions": [{"col": "userid"}]
          },
          {
            "kind": "getgroupeq",
            "name": "ByUserid",
            "conditions": [{"col": "userid"}],
            "order": [{"col": "time", "dir": "DESC"}]
          }
        ]
      }
    }
  }
}
