          },
          {
            "kind": "deleq",
            "name": "ByID",
            "conditions": [{"col": "sessionid"}]
          },
          {
            "kind": "deleq",
            "name": "ByIDs",
            "conditions": [{"col": "sessionid", "cond": "in"}]
          },
          {
